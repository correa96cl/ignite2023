{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "@rocketseat/eslint-config/node"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error"
           
        ], "camelcase": "off"
    }
}